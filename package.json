{
  "name": "RNNBoilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "node ./script/postInstall",
    "start": "export RN_FLAVOUR='' && react-native start",
    "start:nocache": "export RN_FLAVOUR='' && react-native start --reset-cache",
    "start:e2e": "export RN_FLAVOUR=E2E && react-native start --reset-cache",
    "ios": "react-native run-ios",
    "android": "cd ./android && ./gradlew app:assembleDebug && ./gradlew installDebug",
    "test": "jest"
  },
  "dependencies": {
    "mobx": "4.15.2",
    "mobx-persist": "0.4.1",
    "mobx-react": "6.1.5",
    "react": "16.9.0",
